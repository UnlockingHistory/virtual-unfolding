{
"compilerOptions": {
  "module": "CommonJS",
  "noImplicitAny": true,
  "removeComments": true,
  "preserveConstEnums": true,
  "strict": true,
  "allowJs": false,
  "lib": ["es6"],
  "target": "es6",
//   "checkJs": true,
  "sourceMap": true,
  "noUnusedLocals": false,
  "strictNullChecks": true,
},
"include": ["src/**/*"],
"exclude": ["node_modules", "spec"]
}